/* Flash sizes
 * 5606S - CodeFlash - 1 Meg
 *       - Dataflash - 64K
 */
flash(R) : ORIGIN = 0x00000000, LENGTH = 0x100000
dflash(R) : ORIGIN = 0x00800000, LENGTH = 0x10000

/* Ram sizes
 * 5606S  SRAM - 48K
 *        GRAM - 160K
 */
ram(RW)  : ORIGIN = 0x40000000, LENGTH = 0x00c000
gram(RW)  : ORIGIN = 0x60000000, LENGTH = 0x00c000