

flash_nvdata(R) : ORIGIN = 0x00000000, LENGTH = 0x00020000

/* MPC55xx Reset Control Word(RCW) */
rcw      : ORIGIN = 0x00020000, LENGTH = 0x8
flash(R) : ORIGIN = 0x00020008, LENGTH = 0x00160000-0x8
/* 5516S, 48K
 * 5517S,5516G,5516E, 64K RAM
 * 5517G,E , 80K RAM
 */
ram(RW)  : ORIGIN = 0x40000000, LENGTH = 0x100000
