

#define _ASSEMBLER_
#include "asm_ppc.h"



ASM_SECTION_TEXT(.text)

.global MM_TlbSetupOne

/* r3 - MAS0 
 * r4 - MAS1
 * r5 - MAS2
 * r6 - MAS3
 */
MM_TlbSetupOne:
    mtspr SPR_MAS0,r3     
    mtspr SPR_MAS1,r4     
    mtspr SPR_MAS2,r5     
    mtspr SPR_MAS3,r6
    msync
    isync
	tlbwe
	isync
	blr	

